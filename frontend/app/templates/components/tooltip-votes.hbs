<div id="tooltip-map">
{{#if isPopulation}}
	<div class="tooltip-row">
		<h6 style="text-align: center;">Sección : {{hoveredSection.section_code}}</h6>
	</div>

	<div class="tooltip-row">
		<span class="tooltip-color-box pri"></span>
		<span class="tooltip-party"> Población</span>
		<span class="tooltip-votes" style="font-weight: bold;">{{hoveredSection.population}}</span>
	</div>
{{else}}
	{{#if isNormal}}
	<div class="tooltip-row">
		<h6 class="tooltip-header">Sección: {{hoveredSection.section_code}} <span class="small-text">{{totalVotes}} Total de votos</span></h6>
	</div>

	<div class="tooltip-row">
		<table class="table votes-table">
			<thead class="votes-table-header">
				<tr>
					<th>PARTIDO</th>
					<th>VOTOS</th>
					<th>PCT. %</th>
				</tr>
			</thead>
			<tbody class="votes-table-content">
				<tr>
					<td>
						<span class="tooltip-color-box" style="{{firstPartyColor}}"></span>
						<span class="tooltip-party">{{firstParty.name}}</span>
					</td>
					<td>{{firstParty.votes}}</td>
					<td class="tooltip-pct">{{firstParty.percentage}} %</td>
				</tr>

				<tr>
					<td>
						<span class="tooltip-color-box" style="{{secondPartyColor}}"></span>
						<span class="tooltip-party">{{secondParty.name}}</span>
					</td>
					<td>{{secondParty.votes}}</td>
					<td class="tooltip-pct">{{secondParty.percentage}} %</td>
				</tr>

				<tr>
					<td>
						<span class="tooltip-color-box" style="{{thirdPartyColor}}"></span>
						<span class="tooltip-party">{{thirdParty.name}}</span>
					</td>
					<td>{{thirdParty.votes}}</td>
					<td class="tooltip-pct">{{thirdParty.percentage}} %</td>
				</tr>

				<tr>
					<td>
						<span class="tooltip-color-box otros"></span>
						<span class="tooltip-party">{{others.name}}</span>
					</td>
					<td>{{others.votes}}</td>
					<td class="tooltip-pct">{{others.percentage}} %</td>
				</tr>
			</tbody>
		</table>
	</div>
	{{/if}}

	{{#if isComparison}}
	<div class="tooltip-row">
		<h6 class="tooltip-header">Sección: {{hoveredSection.section_code}} <span class="small-text">{{totalVotes}} Total de votos</span></h6>
	</div>


	<div class="tooltip-row pct-bar-labels">
		<p>{{firstParty.name}} {{firstParty.percentage}}%</p>
		<p style="float: right;">{{secondParty.name}} {{secondParty.percentage}}%</p>
	</div>
	<div class="tooltip-row">
		<div class="pct-bar" style={{comparisonBar}}></div>
	</div>

	<div class="tooltip-row">
		<table class="table votes-table">
			<thead class="votes-table-header">
				<tr>
					<th>PARTIDO</th>
					<th>VOTOS</th>
					<th>PCT. %</th>
				</tr>
			</thead>
			<tbody class="votes-table-content">
				<tr>
					<td>
						<span class="tooltip-color-box" style="{{firstPartyColor}}"></span>
						<span class="tooltip-party">{{firstParty.name}}</span>
					</td>
					<td>{{firstParty.votes}}</td>
					<td class="tooltip-pct">{{firstParty.percentage}} %</td>
				</tr>

				<tr>
					<td>
						<span class="tooltip-color-box" style="{{secondPartyColor}}"></span>
						<span class="tooltip-party">{{secondParty.name}}</span>
					</td>
					<td>{{secondParty.votes}}</td>
					<td class="tooltip-pct">{{secondParty.percentage}} %</td>
				</tr>
			</tbody>
		</table>
	</div>

	{{/if}}

	{{#if isSingle}}
	<div class="tooltip-row">
		<h6 class="tooltip-header">Sección: {{hoveredSection.section_code}} <span class="small-text">{{totalVotes}} Total de votos</span></h6>
	</div>
	{{/if}}

{{/if}}
</div>

	
{{#if isPopulation}}
<div class="map-key">
	<div class="key-row-header">
		<div class="key-left-label">
			Baja Población
		</div>
		<span class="small-vertical-line vertical-line-left-pos"></span>
		<div class="key-right-label">
			Alta población
		</div>
		<span class="small-vertical-line vertical-line-right-pos"></span>
	</div>

	<div class="key-row">
		<div class="key-row-label"></div>
		<div class="colors-row">
			<div class="color-box population-25"></div>
			<div class="color-box population-50"></div>
			<div class="color-box population-75"></div>
			<div class="color-box population-100"></div>
		</div>
	</div>
</div>
{{else}}
	{{map-key}}
{{/if}}